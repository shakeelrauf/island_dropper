  line-height: 18px;
  margin: 0;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.card-dl .dl-block:before {
  background-color: #70af44;
}
.card-dl .card-header .dl-block-f:before {
  content: "";
  display: inline-block;
  width: 0;
  height: 0;
  width: 0;
  height: 0;
  border-left: 0.4rem solid transparent;
  border-right: 0.4rem solid transparent;
  border-top: 0.55rem solid #70af44;
  position: absolute;
  left: 28px;
  top: 20px;
  -webkit-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
  transform: rotate(180deg);
}
.card-dl .card-header.collapsed .dl-block-f:before {
  content: "";
  display: inline-block;
  width: 0;
  height: 0;
  width: 0;
  height: 0;
  border-left: 0.4rem solid transparent;
  border-right: 0.4rem solid transparent;
  border-top: 0.55rem solid #70af44;
  position: absolute;
  left: 28px;
  top: 20px;
  -webkit-transform: rotate(0deg);
  -ms-transform: rotate(0deg);
  transform: rotate(0deg);
}
.card-dl .dl-block-f {
  padding-left: 19px;
}
.driver-detail {
  background-color: #70af44;
  border-radius: 0px 0px 22px 22px;
  min-height: 90px;
  padding: 0.55rem 1rem;
}
.driver-detail span {
  color: #fff;
  font-weight: 700;
  font-size: 0.937rem;
  line-height: 18px;
  letter-spacing: 0.03rem;
}
.driver-detail .driver-img span {
  width: 60px;
  height: 60px;
  min-width: 60px;
  border: 1px dashed #fff;
  border-radius: 50%;
  overflow: hidden;
  padding: 1px;
}
.driver-detail .driver-img span img {
  flex-shrink: 0;
}
.track-badge {
  background-color: #70af44;
  color: #fff;
  text-transform: uppercase;
  padding: 3px 10px;
  border-radius: 20px;
  line-height: 16px;
  font-size: 0.8rem;
}
.dl-status:after {
  content: '';
  width: 20px;
  height: 9px;
  border-radius: 22px;
  background-color: #ccc;
  display: inline-block;
  position: absolute;
  right: 0;
  top: 5px;
}
.dl-status.dl-status-cancel:after {
  background-color: #ed1c24;
}
.dl-status.dl-status-complete:after {
  background-color: #70af44;
}
.dl-status.dl-status-enroute:after {
  background-color: #fff100;
} 
.card-dl .dl-block-6 .dl-text {
  font-size: 0.8rem;
  position: relative;
  width: 100%;
  padding-right: 22px;
}
.card-dl .dl-block-6:before {
  margin-right: 8px;
}
.form-group-id .form-label {
  font-size: 1.05rem;
